---
import OO from "../assets/OO.webp";
import BigCard from "../components/BigCard.astro";
import Heading from "../components/Heading.astro";
import Layout from "../layouts/Layout.astro";
import "../Styles/general.css";
import FeatureSection from "../components/FeatureSection.astro";
import BannerSection from "../components/BannerSection.astro";
import MIS from "../assets/MIS.webp";


---
	<Layout>		
		<FeatureSection />
		<BannerSection />
		<Heading />
		<BigCard
  placeholder={OO}
  buttonText="OM YCDK"
  order={1}
  title="OM YOUNG CREATIVES DENMARK"
  text="Young Creative Denmark er et kreativt bureau, der udvikler kampagner og kommunikation med fokus på unge målgrupper. "
/>

<BigCard
  placeholder={MIS}
  buttonText="LÆS MERE"
  order={-1}
  title="VORES MISSION"
  text="Vi kombinerer indsigt, strategi og kreativitet for at skabe autentiske fortællinger, der engagerer og gør en forskel. Vores arbejde bygger på ærlighed, innovation og forståelsen af, hvad der virkelig bevæger unge i dag.
"
/>


		
	</Layout>    

